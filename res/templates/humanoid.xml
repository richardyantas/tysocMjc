        <motor name="mjcact_###_right_elbow"     gear="200" joint="mjcjoint_###_right_elbow"     ctrlrange="-0.4 .4" ctrllimited="true" />
        <motor name="mjcact_###_left_shoulder1"  gear="100" joint="mjcjoint_###_left_shoulder1"  ctrlrange="-0.4 .4" ctrllimited="true" />
