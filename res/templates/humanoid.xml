                    <geom condim="1" material="matgeom" name="mjcgeom_###_right_larm" type="capsule" fromto="0.01 0.01 0.01 .17 .17 .17"  size="0.031" />
                    <geom condim="1" material="matgeom" name="mjcgeom_###_right_hand" type="sphere" pos=".18 .18 .18"  size="0.04"/>
                </body>
            </body>
            <body name="mjcbody_###_left_upper_arm" pos="0 0.17 0.06" >
                <joint name="mjcjoint_###_left_shoulder1" type="hinge" pos="0 0 0" axis="2 -1 1" range="-60 85" stiffness="1" armature="0.0068" limited="true" damping="1" />
