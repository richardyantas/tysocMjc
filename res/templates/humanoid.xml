                                <geom condim="1" material="matgeom" name="mjcgeom_###_right_foot_cap2" type="capsule" fromto="-0.07 0 0 0.14  0.02 0"  size="0.027" />
                            </body>
                        </body>
                    </body>
                    <body name="mjcbody_###_left_thigh" pos="0 0.1 -0.04" >
                        <joint name="mjcjoint_###_left_hip_x" type="hinge" pos="0 0 0" axis="-1 0 0" range="-25 5"  damping="5" stiffness="10" armature="0.01" limited="true" />
                        <joint name="mjcjoint_###_left_hip_z" type="hinge" pos="0 0 0" axis="0 0 -1" range="-60 35" damping="5" stiffness="10" armature="0.01" limited="true" />
                        <joint name="mjcjoint_###_left_hip_y" type="hinge" pos="0 0 0" axis="0 1 0" range="-120 20" damping="5" stiffness="20" armature="0.01" limited="true" />
                        <geom condim="1" material="matgeom" name="mjcgeom_###_left_thigh1" type="capsule" fromto="0 0 0 0 -0.01 -0.34"  size="0.06" />
                        <body name="mjcbody_###_left_shin" pos="0 -0.01 -0.403" >
                            <joint name="mjcjoint_###_left_knee" type="hinge" pos="0 0 .02" axis="0 -1 0" range="-160 -2" stiffness="1" armature="0.0060" limited="true" damping="1" />
                            <geom condim="1" material="matgeom" name="mjcgeom_###_left_shin1" type="capsule" fromto="0 0 0 0 0 -0.3"   size="0.049" />
                            <body name="mjcbody_###_left_foot" pos="0 0 -0.39" >
                                <joint name="mjcjoint_###_left_ankle_y" type="hinge" pos="0 0 0.08" axis="0 1 0"   range="-50 50"  stiffness="4" armature="0.0008" limited="true" damping="1" />
