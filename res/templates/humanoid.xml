                                <geom condim="1" material="matgeom" name="mjcgeom_###_left_foot_cap2" type="capsule" fromto="-0.07 0 0 0.14  -0.02 0"  size="0.027" />
                            </body>
                        </body>
                    </body>
                </body>
            </body>
            <body name="mjcbody_###_right_upper_arm" pos="0 -0.17 0.06" >
