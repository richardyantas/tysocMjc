                    <joint name="mjcjoint_###_left_elbow" type="hinge" pos="0 0 0" axis="0 -1 -1" range="-90 50" stiffness="0" armature="0.0028" limited="true" damping="1" />
                    <geom condim="1" material="matgeom" name="mjcgeom_###_left_larm" type="capsule" fromto="0.01 -0.01 0.01 .17 -0.17 .17"  size="0.031" />
                    <geom condim="1" material="matgeom" name="mjcgeom_###_left_hand" type="sphere" pos=".18 -0.18 .18"  size="0.04"/>
                </body>
            </body>
        </body>
    </worldbody>

    <actuator>
        <motor name="mjcact_###_abdomen_y"       gear="200" joint="mjcjoint_###_abdomen_y"       ctrlrange="-0.4 .4" ctrllimited="true" />
        <motor name="mjcact_###_abdomen_z"       gear="200" joint="mjcjoint_###_abdomen_z"       ctrlrange="-0.4 .4" ctrllimited="true" />
        <motor name="mjcact_###_abdomen_x"       gear="200" joint="mjcjoint_###_abdomen_x"       ctrlrange="-0.4 .4" ctrllimited="true" />
