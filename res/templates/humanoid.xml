        <motor name="mjcact_###_left_hip_y"      gear="600" joint="mjcjoint_###_left_hip_y"      ctrlrange="-0.4 .4" ctrllimited="true" />
        <motor name="mjcact_###_left_knee"       gear="400" joint="mjcjoint_###_left_knee"       ctrlrange="-0.4 .4" ctrllimited="true" />
