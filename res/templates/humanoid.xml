<mujoco model="Humanoid">

    <worldbody>
        <!--<geom name="mjcgeom_###_floor" pos="0 0 0" size="0 0 .25" type="plane" material="matplane" condim="3"/>-->

        <light directional="false" diffuse=".2 .2 .2" specular="0 0 0" pos="0 0 5" dir="0 0 -1" castshadow="false"/>
        <light mode="targetbodycom" target="mjcbody_###_tmjcroot" directional="false" diffuse=".8 .8 .8" specular="0.3 0.3 0.3" pos="0 0 4.0" dir="0 0 -1"/>

        <body name="mjcbody_###_tmjcroot" pos="0 0 1.4">
