                        <body name="mjcbody_###_right_shin" pos="0 0.01 -0.403" >
                            <joint name="mjcjoint_###_right_knee" type="hinge" pos="0 0 .02" axis="0 -1 0" range="-160 -2" stiffness="1" armature="0.0060" limited="true" damping="1" />
                            <geom condim="1" material="matgeom" name="mjcgeom_###_right_shin1" type="capsule" fromto="0 0 0 0 0 -0.3"   size="0.049" />
                            <body name="mjcbody_###_right_foot" pos="0 0 -0.39" >
