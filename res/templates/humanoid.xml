        <motor name="mjcact_###_right_hip_y"     gear="600" joint="mjcjoint_###_right_hip_y"     ctrlrange="-0.4 .4" ctrllimited="true" />
        <motor name="mjcact_###_right_knee"      gear="400" joint="mjcjoint_###_right_knee"      ctrlrange="-0.4 .4" ctrllimited="true" />
