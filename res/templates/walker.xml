<mujoco model="template_walker">

  <worldbody>
    <!--<geom name="mjcgeom_###_floor" type="plane" contype="1" conaffinity="1" pos="0 0 0" size="1 1 .2" material="grid" zaxis="0 0 1" friction=".7 .1 .1"/>-->
    <body name="mjcbody_###_tmjcroot" pos="0 0 1.3" >
      <light name="mjclight_###_light" pos="0 0 2" mode="trackcom"/>
      <camera name="mjccam_###_side" pos="0 -2 .7" euler="60 0 0" mode="trackcom"/>
      <camera name="mjccam_###_back" pos="-2 0 .5" xyaxes="0 -1 0 1 0 3" mode="trackcom"/>
      <joint name="mjcjoint_###_tmjcrootz" axis="0 0 1" type="slide" limited="false" armature="0" damping="0" solimplimit="0 .99 .01"/>
      <joint name="mjcjoint_###_tmjcrootx" axis="1 0 0" type="slide" limited="false" armature="0" damping="0" solimplimit="0 .99 .01"/>
      <joint name="mjcjoint_###_tmjcrooty" axis="0 1 0" type="hinge" limited="false" armature="0" damping="0" solimplimit="0 .99 .01"/>
      <geom name="mjcgeom_###_tmjcroot" material="self" type="capsule" size="0.07 0.3" contype="1" conaffinity="0" friction=".7 .1 .1"/>
      <body name="mjcbody_###_right_thigh" pos="0 -.05 -0.3">
        <joint name="mjcjoint_###_right_hip" axis="0 -1 0" range="-20 100" damping=".1" armature="0.01" limited="true" solimplimit="0 .99 .01"/>
        <geom name="mjcgeom_###_right_thigh" material="self" type="capsule" pos="0 0 -0.225" size="0.05 0.225" contype="1" conaffinity="0" friction=".7 .1 .1"/>
        <body name="mjcbody_###_right_leg" pos="0 0 -0.7">
          <joint name="mjcjoint_###_right_knee" axis="0 -1 0" pos="0 0 0.25" range="-150 0" damping=".1" armature="0.01" limited="true" solimplimit="0 .99 .01"/>
          <geom name="mjcgeom_###_right_leg" material="self" type="capsule" size="0.04 0.25" contype="1" conaffinity="0" friction=".7 .1 .1"/>
          <body name="mjcbody_###_right_foot" pos="0.06 0 -0.25">
            <joint name="mjcjoint_###_right_ankle" axis="0 -1 0" pos="-0.06 0 0" range="-45 45" damping=".1" armature="0.01" limited="true" solimplimit="0 .99 .01"/>
            <geom name="mjcgeom_###_right_foot" material="self" type="capsule" zaxis="1 0 0" size="0.05 0.1" contype="1" conaffinity="0" friction=".7 .1 .1"/>
          </body>
        </body>
      </body>
      <body name="mjcbody_###_left_thigh" pos="0 .05 -0.3" >
        <joint name="mjcjoint_###_left_hip" axis="0 -1 0" range="-20 100" damping=".1" armature="0.01" limited="true" solimplimit="0 .99 .01"/>
        <geom name="mjcgeom_###_left_thigh" material="self" type="capsule" pos="0 0 -0.225" size="0.05 0.225" contype="1" conaffinity="0" friction=".7 .1 .1"/>
        <body name="mjcbody_###_left_leg" pos="0 0 -0.7">
          <joint name="mjcjoint_###_left_knee" axis="0 -1 0" pos="0 0 0.25" range="-150 0" damping=".1" armature="0.01" limited="true" solimplimit="0 .99 .01"/>
          <geom name="mjcgeom_###_left_leg" material="self" type="capsule" size="0.04 0.25" contype="1" conaffinity="0" friction=".7 .1 .1"/>
          <body name="mjcbody_###_left_foot" pos="0.06 0 -0.25">
            <joint name="mjcjoint_###_left_ankle" axis="0 -1 0" pos="-0.06 0 0" range="-45 45" damping=".1" armature="0.01" limited="true" solimplimit="0 .99 .01"/>
            <geom name="mjcgeom_###_left_foot" material="self" type="capsule" zaxis="1 0 0" size="0.05 0.1" contype="1" conaffinity="0" friction=".7 .1 .1"/>
          </body>
        </body>
      </body>
    </body>
  </worldbody>

  <sensor>
    <subtreelinvel name="mjcsensor_###_tmjcroot_subtreelinvel" body="mjcbody_###_tmjcroot"/>
  </sensor>

  <actuator>
    <motor name="mjcact_###_right_hip" joint="mjcjoint_###_right_hip" gear="100" ctrlrange="-1 1" ctrllimited="true"/>
    <motor name="mjcact_###_right_knee" joint="mjcjoint_###_right_knee" gear="50" ctrlrange="-1 1" ctrllimited="true"/>
    <motor name="mjcact_###_right_ankle" joint="mjcjoint_###_right_ankle" gear="20" ctrlrange="-1 1" ctrllimited="true"/>
    <motor name="mjcact_###_left_hip" joint="mjcjoint_###_left_hip" gear="100" ctrlrange="-1 1" ctrllimited="true"/>
    <motor name="mjcact_###_left_knee" joint="mjcjoint_###_left_knee" gear="50" ctrlrange="-1 1" ctrllimited="true"/>
    <motor name="mjcact_###_left_ankle" joint="mjcjoint_###_left_ankle" gear="20" ctrlrange="-1 1" ctrllimited="true"/>
  </actuator>

</mujoco>